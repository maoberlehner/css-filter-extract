{
  "name": "css-node-extract",
  "description": "Extract certain nodes from CSS code",
  "keywords": [
    "CSS"
  ],
  "version": "2.1.1",
  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/css-node-extract",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc -w",
    "clean": "rimraf dist",
    "lint": "tslint src/**/*.ts",
    "test": "jest test/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maoberlehner/css-node-extract"
  },
  "bugs": {
    "url": "https://github.com/maoberlehner/css-node-extract/issues"
  },
  "dependencies": {
    "change-case": "^3.0.1",
    "postcss": "^6.0.13"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "jest": "^21.2.1",
    "postcss-less": "^1.1.1",
    "postcss-scss": "^1.0.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.1.4",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$"
  }
}
